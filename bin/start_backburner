#!/usr/bin/env ruby

require 'rubygems'
require 'backburner'
ENV['RAILS_ENV'] = 'production'
dir = File.dirname(__FILE__).to_s
parts = dir.split("/")
parts.pop
dir = parts.join("/")
require File.join(dir,'config', 'application')
STDERR.puts "Backburner.work"
Contact::Application.initialize!
Backburner.work
